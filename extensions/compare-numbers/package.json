{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "compare-numbers",
  "title": "Compare Numbers",
  "description": "Compare two numbers.",
  "icon": "extension_icon.png",
  "author": "bailey_jennings",
  "categories": ["Data"],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Caculate Percentage Difference",
      "description": "Caculate the percentage difference of two numbers.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}